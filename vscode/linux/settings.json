{
    "cortex-debug.enableTelemetry": false,
    "cortex-debug.JLinkGDBServerPath": "${workspaceRoot}/tools/linux/jlink/JLinkGDBServer",
    "cortex-debug.armToolchainPath": "${workspaceRoot}/tools/linux/gcc-arm-none-eabi/bin",
    "C_Cpp.clang_format_fallbackStyle": "Google",
    "C_Cpp.clang_format_path": "/usr/bin/clang-format",
    "C_Cpp.clang_format_style": "file",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "editor.fontSize": 12,
    "editor.tabSize": 2,
    "terminal.integrated.fontFamily": "Courier New",
    "editor.minimap.enabled": false,
    "workbench.colorTheme": "Visual Studio Light",
    "gnuGlobal.gtagsForceCpp": "Enabled",
    "update.showReleaseNotes": false,
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.cStandard": "c11",
    "window.zoomLevel": 0,
}
