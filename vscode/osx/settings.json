{
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4, TabWidth: 4, ColumnLimit: 100}",
    "C_Cpp.clang_format_path": "/usr/local/bin/clang-format",
    "C_Cpp.clang_format_style": "file",
    "C_Cpp.default.cStandard": "c11",
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.intelliSenseMode": "gcc-x64",
    "C_Cpp.intelliSenseCacheSize": 511,
    "C_Cpp.updateChannel": "Insiders",
    "C_Cpp.workspaceParsingPriority": "low",
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "cmake.buildDirectory": "${workspaceFolder}/BUILD",
    "cmake.buildTask": true,
    "cmake.configureOnOpen": false,
    "cmake.configureSettings": {
        "CMAKE_COLOR_MAKEFILE": "off"
    },
    "cmake.defaultVariants": {},
    "cmake.generator": "Ninja",
    "cmake.useCMakeServer": false,
    "cortex-debug.enableTelemetry": false,
    "editor.autoClosingOvertype": "always",
    "editor.detectIndentation": false,
    "editor.fontSize": 12,
    "editor.highlightActiveIndentGuide": false,
    "editor.minimap.enabled": false,
    "editor.occurrencesHighlight": false,
    "editor.renderIndentGuides": false,
    "editor.renderWhitespace": "none",
    "editor.rulers": [
        100
    ],
    "editor.showFoldingControls": "always",
    "editor.tabSize": 4,
    "editor.wrappingIndent": "indent",
    "explorer.autoReveal": false,
    "explorer.compactFolders": false,
    "explorer.confirmDragAndDrop": false,
    //"explorer.decorations.badges": false,
    "files.associations": {
        "*.gni": "gn",
        "*.rst": "restructuredtext"
    },
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "gnformat.path": "/Users/palchak/.local/bin/gn",
    "kconfig.zephyr.base": "/usr/local/google/home/palchak/projects/pointy/boosty/third_party/nordic/ncs_v1.5.1/zephyr",
    "problems.decorations.enabled": false,
    "python.defaultInterpreterPath": "/usr/bin/python3",
    "python.pythonPath": "python3",
    "remote.SSH.configFile": "~/.ssh/config",
    "remote.SSH.connectTimeout": 30,
    "remote.SSH.path": "/usr/local/bin/ssh",
    "remote.SSH.remotePlatform": {
        "palchak.mtv.corp.google.com": "linux"
    },
    "remote.SSH.useLocalServer": false,
    "scm.countBadge": "off",
    "scm.defaultViewMode": "tree",
    "search.useIgnoreFiles": false,
    "security.workspace.trust.untrustedFiles": "open",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.fontFamily": "Courier New",
    "update.showReleaseNotes": false,
    "window.restoreWindows": "none",
    "window.title": "${dirty}${activeEditorMedium}",
    "window.zoomLevel": 0,
    "workbench.colorCustomizations": {
        "editor.findMatchBackground": "#288f8fa2", //Current SEARCH MATCH
        "editor.findMatchHighlightBackground": "#0c88088c", //Other SEARCH MATCHES
        "editor.lineHighlightBackground": "#303040",
        "editor.selectionBackground": "#1273c2a1", //Currently SELECTED text
        "editor.selectionHighlightBackground": "#01832256", //Same content as selection
        "editor.selectionHighlightBorder": "#FFFA",
        "editorGutter.background": "#040404",
        "gitDecoration.ignoredResourceForeground": "#a4a4a4",
        "numberedBookmarks.lineBackground": "#007700"
    },
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "workbench.editor.focusRecentEditorAfterClose": false,
    "workbench.editor.closeEmptyGroups": false,
    "workbench.startupEditor": "none",
    "workbench.tree.indent": 16,
    "workbench.tree.renderIndentGuides": "always",
    "workbench.colorTheme": "Solarized Dark+",
    "redhat.telemetry.enabled": false,
    "kconfig.zephyr.board": {
        "board": "pca10040"
    },
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.suggest.showWords": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false
}
