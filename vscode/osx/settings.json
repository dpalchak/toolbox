{
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4, TabWidth: 4, ColumnLimit: 100}",
    "C_Cpp.clang_format_path": "/usr/local/bin/clang-format",
    "C_Cpp.clang_format_style": "file",
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.cStandard": "c11",
    "C_Cpp.default.intelliSenseMode": "gcc-x64",
    "C_Cpp.intelliSenseCacheSize": 511,
    "C_Cpp.updateChannel": "Insiders",
    "C_Cpp.workspaceParsingPriority": "low",
    "cmake.buildDirectory": "${workspaceFolder}/BUILD",
    "cmake.configureSettings": {"CMAKE_COLOR_MAKEFILE":"off"},
    "cmake.generator": "Ninja",
    "cmake.useCMakeServer": false,
    "cortex-debug.enableTelemetry": false,
    "editor.autoClosingOvertype": "always",
    "editor.detectIndentation": false,
    "editor.fontSize": 12,
    "editor.highlightActiveIndentGuide": false,
    "editor.minimap.enabled": false,
    "editor.renderIndentGuides": false,
    "editor.rulers": [100],
    "editor.showFoldingControls": "always",
    "editor.tabSize": 4,
    "editor.wrappingIndent": "indent",
    "explorer.confirmDragAndDrop": false,
    "files.trimTrailingWhitespace": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.fontFamily": "Courier New",
    "update.showReleaseNotes": false,
    "window.title": "${dirty}${activeEditorMedium}",
    "window.zoomLevel": 0,
    //"workbench.colorCustomizations": {
        //"editorGutter.background": "#e0e0e0",
    //},
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.tree.indent": 16,
    "workbench.tree.renderIndentGuides": "always",
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "cmake.configureOnOpen": false,
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "cmake.buildTask": true,
    "cmake.defaultVariants": {},

    "files.associations": {
        "*.gni": "gn",
    },

    "gnformat.path": "/Users/palchak/.local/bin/gn",
    "[gn]":{
        "editor.tabSize": 2
    },
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "workbench.colorTheme": "Solarized Dark+",
    "scm.defaultViewMode": "tree",
    "scm.countBadge": "off"
}
